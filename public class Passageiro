package unicesumar.segundoBimestre;

public class Passageiro {
     private String nome;
     private String RG;
     
     public String getNome() {
	 return nome;
     }
     public void setNome(String nome) {
	 this.nome = nome;
     }
     public String getRG() {
	 return RG;
     }
     public void setRG(String rG) {
	 this.RG = rG;
     }
     
}
